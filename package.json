    "build:push": "ng build --configuration production --base-href /unimart-frontend/;  git add docs/;  git commit -m 'Updated docs'; git push -u origin main",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
